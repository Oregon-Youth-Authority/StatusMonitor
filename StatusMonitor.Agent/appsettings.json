{
   "Logging": {
      "LogLevel": {
         "Default": "Information",
         "System": "Information",
         "Microsoft": "Information"
      }
   },
   "ApiUrl": "https://oya-status-monitor.herokuapp.com/", // API URL
   "ConfigurationRefreshInterval": 900000, // How often (in milliseconds) to refresh monitor configuration - 15 minutes
   "StatusCheckInterval": 300000, // How often (in milliseconds) to check for a status change - 5 minutes
   "StatusUpdateInterval": 3600000 // How often (in milliseconds) to update status even when there is no status change - 60 minutes
}
